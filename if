package com.company;

public class Main {

    public static void main(String[] args) {
        int myInt = 20;
        if (myInt > 25) {
            ;
            System.out.print("да, верно.");
        } else if (myInt < 15) {
            System.out.println("нет, не вурно.");
        } else {
            System.out.println("не один из предыдущих.");
        }
    }
}
